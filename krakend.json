{
    "$schema": "https://www.krakend.io/schema/krakend.json",
    "version": 3,
  
    "port": 8081,
    "extra_config": {
      "router": {
        "return_error_msg": true
      },
      "telemetry/opentelemetry": {
        "metric_reporting_period": 0,
        "exporters": {
          "prometheus": [
            {
              "port": 9091,
              "name": "krakend"
            }
          ]
        }
      },
      "security/cors": {
        "allow_origins": ["*"],
        "allow_methods": ["POST", "GET", "HEAD", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "expose_headers": ["Content-Length", "Content-Type", "Authorization", "Cookie", "X-Martian"],
        "max_age": "5m",
        "allow_headers": ["Content-Length", "Content-Type", "Authorization", "Cookie", "X-Martian"],
        "allow_credentials": true,
        "debug": true
      }
    },
    "name": "KrakenD - API Gateway",
    "timeout": "3000ms",
    "endpoints":],
    "output_encoding": "json"
  }
  